{
  "name": "@maskjson/core",
  "version": "1.0.0",
  "description": "Fast, zero-dependency JSON redaction and masking engine",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Avinash1721/maskjson-core.git"
  },
  "bugs": {
    "url": "https://github.com/Avinash1721/maskjson-core/issues"
  },
  "homepage": "https://maskjson.com",
  "engines": {
    "node": ">=18"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean --minify",
    "test": "vitest run",
    "lint": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "json",
    "redact",
    "mask",
    "pii",
    "privacy",
    "security"
  ],
  "author": "MaskJSON",
  "license": "MIT",
  "devDependencies": {
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1",
    "tsx": "^4.7.1"
  }
}