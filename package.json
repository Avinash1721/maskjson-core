{
  "name": "@maskjson/core",
  "version": "1.0.0",
  "description": "Fast, zero-dependency JSON redaction and masking engine",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "npx tsup src/index.ts --watch --dts",
    "build": "npx tsup src/index.ts --format cjs,esm --dts --clean --minify",
    "test": "npx vitest run",
    "test:watch": "npx vitest",
    "benchmark": "npx tsx benchmark/benchmark.ts",
    "lint": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "json",
    "redact",
    "mask",
    "security",
    "pii",
    "privacy",
    "typescript"
  ],
  "author": "MaskJSON",
  "license": "MIT",
  "devDependencies": {
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1",
    "tsx": "^4.7.1"
  }
}